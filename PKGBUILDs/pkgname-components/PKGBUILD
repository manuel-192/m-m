#!/bin/bash

# Maintainer: manuel (see forum.endeavouros.com)

pkgname=pkgname-components
pkgdesc="Show parts of a package name."
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1
pkgrel=1.1

arch=('any')
license=('GPL')

source=(
    $url/$pkgname
)
sha512sums=('f22072daefd4dde6c3c538e0b7481e88de7517231feaca8808f8e9dfa8299c0a5c6192a50034ac04f7010ff34ee52706336f4eee65ecf045cc2ba63b86813ebc')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
