#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=eos-timediff
pkgdesc="Simple app to show time difference"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=2021.09.25
pkgrel=1

arch=('any')
license=('GPL')
depends=('bash')

source=(
    $url/$pkgname
)
sha512sums=('1d22df31f297a1855b601a3d6497b713ea8c123b0d2d1842650db3bb2a89308cdd474f08a61777ce53a2c246b40f0c597eb737117117a4ae97e9a91e0562b442')

package() {
  install -Dm755 $pkgname                    $pkgdir/usr/bin/$pkgname
}
