    #https://documentation.libreoffice.org/en/english-documentation
    /usr/lib/libreoffice/help/index.html
)
lua=(           file:///usr/share/doc/lua52/manual.html)
nano=(          file:///usr/share/doc/nano/nano.html)
nanorc=(        file:///usr/share/doc/nano/nanorc.5.html)
