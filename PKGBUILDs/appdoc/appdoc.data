#!/bin/bash
#
# Database file for appdoc.
#

# Collect all app names (or package names) in this list.

Items=(
    bash
    bashdb
    bashref
    code
    fox
    geany
    ghostscript
    imagemagick
    libreoffice
    lua
    nano
    nanorc
    nvidia
    pandoc
    qpdf
    virtualbox
)

# Collect app specific URLs (local or remote) here.
# If both URLs exist, add the local URL as second.

bash=(          /usr/share/doc/bash/bash.html)
