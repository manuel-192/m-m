#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=GitUpdate
pkgdesc="Update special repo"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1.1
pkgrel=1
arch=('any')
license=('custom')
depends=(bash)

source=(
    $pkgname
)
sha512sums=('a5b04a07d21b762019ce242d40cb76b19226ef419533411e5dbf53b0fcc613942e2a3865a632a4769438d8d5da2389af00a3fb5dc7e79419a0f7a50f0ba01e6b')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
