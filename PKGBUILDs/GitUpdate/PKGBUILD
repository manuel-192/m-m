#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=GitUpdate
pkgdesc="Update special repo"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1.2
pkgrel=1
arch=('any')
license=('custom')
depends=(bash)

source=(
    $pkgname
)
sha512sums=('47548361ceb9ef727c5d5d2314b352af9b6c3fd0462e48f8f0030e3ce213c0964ed938fcd1d871584dc5c513e90485b7bd803ed010c5beca13fd8f1b245046ae')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
