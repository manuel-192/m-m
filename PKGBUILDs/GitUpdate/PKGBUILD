#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=GitUpdate
pkgdesc="Update special repo"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1.3
pkgrel=1
arch=('any')
license=('custom')
depends=(bash)

source=(
    $pkgname
)
sha512sums=('78f74f693f1278b351dd1a6ffec547f637dfd44c185d04b63a96cc9db613a83b16540062eff080071a697b9018d08e88ddee8035b00369dc4c554466aa8aaf9a')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
