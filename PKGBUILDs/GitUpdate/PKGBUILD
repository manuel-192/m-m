#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=GitUpdate
pkgdesc="Update special repo"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1
pkgrel=1
arch=('any')
license=('custom')
depends=(bash)

source=(
    $pkgname
)
sha512sums=('2a068232cdb33de9bc162b8b8458ce243bd0083c15f3d2832dea33b13d0e0c5bb21175b50e62031f029cbdc1d38e2f536803f4e3fe94b001d8220d187d71b7ba')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
