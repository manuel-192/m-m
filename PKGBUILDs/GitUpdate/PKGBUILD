#!/bin/bash
# Maintainer: manuel (see forum.endeavouros.com)

pkgname=GitUpdate
pkgdesc="Update special repo"
url="https://github.com/manuel-192/m-m/raw/master/PKGBUILDs/$pkgname"
pkgver=1.3.1
pkgrel=1
arch=('any')
license=('custom')
depends=(bash)

source=(
    $pkgname
)
sha512sums=('18eb1baa99934bffb78fb15ab06961ec531050fe3dd1315106d6ee0f8f35192dad697893adb2416f156a393edfee93a3a246f69f12879d19a8cf0cd5f691049e')

package() {
    install -Dm755 $pkgname   $pkgdir/usr/bin/$pkgname
}
